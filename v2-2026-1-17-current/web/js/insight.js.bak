        // æ¸²æŸ“è§†é¢‘æ’­æ”¾é‡è¶‹åŠ¿å›¾åˆ°æŒ‡å®š canvasï¼ˆæ¨¡å¼4ï¼‰
        function _renderViewsTrendChartToCanvas(canvasId, videos) {
            const canvas = document.getElementById(canvasId);
            if (!canvas || !videos?.length) return false;

            // æ ¼å¼åŒ–æ•°å­—
            function formatNum(num) {
                if (!num || num === 0) return '0';
                if (num >= 100000000) return (num / 100000000).toFixed(1) + 'äº¿';
                if (num >= 10000) return (num / 10000).toFixed(1) + 'ä¸‡';
                if (num >= 1000) return (num / 1000).toFixed(1) + 'K';
                return num.toString();
            }

            // è·å–æ—¶é—´æ•°æ®
            const dates = videos.map(v => v.published_at).filter(Boolean).sort();
            if (dates.length === 0) return false;

            const firstDate = new Date(dates[0]);
            const lastDate = new Date(dates[dates.length - 1]);
            const daySpan = Math.ceil((lastDate - firstDate) / (1000 * 60 * 60 * 24));

            // é€‰æ‹©ç²’åº¦
            let getTimeKey, formatLabel, granularityLabel;
            if (daySpan < 45) {
                getTimeKey = (d) => d.slice(0, 10);
                formatLabel = (k) => k.slice(5).replace('-', '/');
                granularityLabel = 'æ—¥';
            } else if (daySpan < 120) {
                getTimeKey = (d) => {
                    const date = new Date(d);
                    const year = date.getFullYear();
                    const week = Math.ceil((date - new Date(year, 0, 1)) / (7 * 24 * 60 * 60 * 1000));
                    return `${year}W${String(week).padStart(2, '0')}`;
                };
                formatLabel = (k) => k.replace('20', '').replace('W', 'å‘¨');
                granularityLabel = 'å‘¨';
            } else {
                getTimeKey = (d) => d.slice(0, 7);
                formatLabel = (k) => k.slice(2).replace('-', '/');
                granularityLabel = 'æœˆ';
            }

            // æŒ‰æ—¶é—´ç²’åº¦èšåˆ
            const timeData = {};
            videos.forEach(v => {
                if (!v.published_at) return;
                const timeKey = getTimeKey(v.published_at);
                if (!timeData[timeKey]) timeData[timeKey] = { views: 0, count: 0 };
                timeData[timeKey].views += v.view_count || 0;
                timeData[timeKey].count += 1;
            });

            const timePoints = Object.keys(timeData).sort().slice(-30);
            const timeLabels = timePoints.map(formatLabel);
            const viewsData = timePoints.map(t => timeData[t].views);
            const countData = timePoints.map(t => timeData[t].count);

            // é”€æ¯æ—§å›¾è¡¨
            if (conclusionChartInstances[canvasId]) {
                conclusionChartInstances[canvasId].destroy();
            }

            conclusionChartInstances[canvasId] = new Chart(canvas, {
                type: 'line',
                data: {
                    labels: timeLabels,
                    datasets: [{
                        label: `${granularityLabel}æ€»æ’­æ”¾é‡`,
                        data: viewsData,
                        borderColor: '#06b6d4',
                        backgroundColor: 'rgba(6, 182, 212, 0.1)',
                        borderWidth: 2,
                        fill: true,
                        tension: 0.3,
                        pointRadius: 3,
                        yAxisID: 'y'
                    }, {
                        label: `${granularityLabel}è§†é¢‘æ•°`,
                        data: countData,
                        borderColor: '#f97316',
                        backgroundColor: 'transparent',
                        borderWidth: 2,
                        borderDash: [5, 5],
                        fill: false,
                        tension: 0.3,
                        pointRadius: 2,
                        yAxisID: 'y1'
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    interaction: { mode: 'index', intersect: false },
                    plugins: {
                        legend: {
                            position: 'top',
                            align: 'end',
                            labels: { color: '#94a3b8', font: { size: 10 }, boxWidth: 10, usePointStyle: true }
                        },
                        tooltip: {
                            callbacks: {
                                label: function(ctx) {
                                    const idx = ctx.dataIndex;
                                    if (ctx.datasetIndex === 0) {
                                        const avgViews = countData[idx] > 0 ? Math.round(viewsData[idx] / countData[idx]) : 0;
                                        return `æ€»æ’­æ”¾: ${formatNum(ctx.raw)} (å‡æ’­: ${formatNum(avgViews)})`;
                                    }
                                    return `è§†é¢‘æ•°: ${ctx.raw}`;
                                }
                            }
                        }
                    },
                    scales: {
                        x: {
                            grid: { color: 'rgba(255,255,255,0.05)' },
                            ticks: { color: '#64748b', autoSkip: true, maxTicksLimit: 10, maxRotation: 45 }
                        },
                        y: {
                            type: 'linear', position: 'left',
                            grid: { color: 'rgba(255,255,255,0.05)' },
                            ticks: { color: '#06b6d4', callback: (v) => formatNum(v) },
                            title: { display: true, text: 'æ€»æ’­æ”¾é‡', color: '#06b6d4' }
