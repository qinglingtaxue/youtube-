                    });
                } else if (text.includes('å‘å¸ƒ') || text.includes('å‘¨')) {
                    actions.push({
                        text: `å‘å¸ƒè®¡åˆ’ï¼š${item.text}`,
                        source: item.source,
                        tabId: getTabIdByName(item.source)
                    });
                }
