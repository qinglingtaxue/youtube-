        /**
         * é€šç”¨æ ‘å›¾/çŸ©å½¢çƒ­åŠ›å›¾
         * @param {string} containerId - å®¹å™¨divçš„ID
         * @param {Array} data - æ•°æ® [{label, value, children?}, ...]
         * @param {Object} config - é…ç½®
         */
        function renderTreemap(containerId, data, config = {}) {
            const container = document.getElementById(containerId);
            if (!container || data.length === 0) return;

            container.innerHTML = '';

            const total = data.reduce((sum, d) => sum + d.value, 0);
            const containerWidth = container.offsetWidth || 400;
            const containerHeight = config.height || 280;

            // ç®€åŒ–çš„Treemapå¸ƒå±€ç®—æ³•
            let x = 0, y = 0;
            let rowHeight = containerHeight;
            let rowWidth = containerWidth;
            let remaining = [...data].sort((a, b) => b.value - a.value);

            const rects = [];
            while (remaining.length > 0) {
                const item = remaining.shift();
                const ratio = item.value / total;
                const area = ratio * containerWidth * containerHeight;

                // è®¡ç®—çŸ©å½¢å°ºå¯¸
                let w, h;
                if (rowWidth > rowHeight) {
                    w = (item.value / total) * containerWidth;
                    h = containerHeight;
                } else {
                    w = containerWidth;
                    h = (item.value / total) * containerHeight;
                }

                // é™åˆ¶æœ€å°å°ºå¯¸
                w = Math.max(w, 40);
                h = Math.max(h, 30);

                rects.push({
                    ...item,
                    x: x % containerWidth,
                    y: Math.floor(x / containerWidth) * 40,
                    w: Math.min(w, containerWidth - (x % containerWidth)),
                    h: Math.min(h, 40),
                    ratio
                });

                x += w;
                if (x >= containerWidth) {
                    x = 0;
                    y += 40;
                }
            }

            // æ¸²æŸ“çŸ©å½¢
            const colors = [
                'rgba(239, 68, 68, 0.8)',
                'rgba(249, 115, 22, 0.8)',
                'rgba(234, 179, 8, 0.8)',
                'rgba(34, 197, 94, 0.8)',
                'rgba(6, 182, 212, 0.8)',
                'rgba(59, 130, 246, 0.8)',
                'rgba(139, 92, 246, 0.8)',
                'rgba(236, 72, 153, 0.8)'
            ];

            const rectsHTML = data.slice(0, 20).map((item, i) => {
                const ratio = item.value / total;
                const width = Math.max(ratio * containerWidth * 2, 60);
                const height = 35;
                const color = item.color || colors[i % colors.length];

                return `<div class="treemap-item" style="
                    display: inline-flex;
                    align-items: center;
                    justify-content: center;
                    width: ${width}px;
                    height: ${height}px;
                    background: ${color};
                    border-radius: 4px;
                    margin: 2px;
                    padding: 4px 8px;
                    font-size: 11px;
                    color: white;
                    cursor: pointer;
                    transition: all 0.2s;
                    overflow: hidden;
                    text-overflow: ellipsis;
                    white-space: nowrap;
                " title="${item.label}: ${config.valueFormatter ? config.valueFormatter(item.value) : item.value}">
                    ${item.label.substring(0, 10)}${item.label.length > 10 ? '...' : ''}
                </div>`;
            }).join('');

            container.innerHTML = `
                <div style="
                    display: flex;
                    flex-wrap: wrap;
                    gap: 2px;
                    padding: 8px;
                    min-height: ${containerHeight}px;
                    align-content: flex-start;
                ">
                    ${rectsHTML}
                </div>
            `;
        }

