        }

        // æ¸²æŸ“æ³¨æ„äº‹é¡¹åˆ—è¡¨
        function renderWarningList(warnings) {
            const container = document.getElementById('reportWarningList');
            if (!container) return;

            const typeIcons = {
                data: 'ğŸ“Š',
                time: 'â°',
