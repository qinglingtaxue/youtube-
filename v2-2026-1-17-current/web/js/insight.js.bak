            if (tabConclusions[tabId]) {
                // æ£€æŸ¥æ˜¯å¦å·²æœ‰ç›¸åŒ patternId çš„ç»“è®ºï¼Œå¦‚æœæœ‰åˆ™æ›¿æ¢ï¼ˆé¿å…é‡å¤ï¼‰
                const existingIndex = tabConclusions[tabId].items.findIndex(item => item.patternId === patternId);

                // å¦‚æœ chartConfig æ˜¯å­—ç¬¦ä¸²ï¼Œä¿å­˜ä¸º sourceCanvasIdï¼ˆç‚¹å‡»å±•å¼€æ—¶å¤ç”¨åŸå›¾è¡¨ï¼‰
                let sourceCanvasId = null;
                let fallbackConfig = null;
                if (typeof chartConfig === 'string') {
                    sourceCanvasId = chartConfig;
                    chartConfig = null;
                } else if (chartConfig && typeof chartConfig === 'object' && chartConfig.sourceCanvasId) {
                    // æ–°æ ¼å¼ï¼š{ sourceCanvasId: 'xxx', fallbackConfig: {...} }
                    sourceCanvasId = chartConfig.sourceCanvasId;
                    fallbackConfig = chartConfig.fallbackConfig || null;
                    chartConfig = fallbackConfig;  // å°† fallbackConfig å­˜ä¸º chartConfigï¼ˆä½œä¸ºå›é€€æ¸²æŸ“æ–¹æ¡ˆï¼‰
                }

                const newItem = {
                    patternId,
                    patternName,
                    dataSource,
                    conclusion,
                    examples: examples || [],
                    chartConfig: chartConfig,
                    sourceCanvasId: sourceCanvasId  // æ–°å¢ï¼šå…³è”åŸå›¾è¡¨çš„ canvas ID
                };

                if (existingIndex !== -1) {
                    // æ›¿æ¢å·²æœ‰ç»“è®ºï¼ˆæ–°æ•°æ®ä¼˜å…ˆï¼Œå¦‚APIæ•°æ®æ›¿æ¢fallbackï¼‰
                    // ä¿ç•™å·²æœ‰çš„ sourceCanvasIdï¼ˆå¦‚æœæ–°çš„æ²¡æœ‰ï¼‰
                    if (!newItem.sourceCanvasId && tabConclusions[tabId].items[existingIndex].sourceCanvasId) {
                        newItem.sourceCanvasId = tabConclusions[tabId].items[existingIndex].sourceCanvasId;
                    }
                    tabConclusions[tabId].items[existingIndex] = newItem;
                    console.log(`[æ¨¡å¼ç»“è®º] ${tabConclusions[tabId].name} - æ¨¡å¼${patternId}ï¼š${patternName} (å·²æ›´æ–°)${sourceCanvasId ? ' (å…³è”å›¾è¡¨:' + sourceCanvasId + ')' : ''}${chartConfig ? ' (å«å›¾è¡¨é…ç½®)' : ''}`);
                } else {
                    tabConclusions[tabId].items.push(newItem);
                    console.log(`[æ¨¡å¼ç»“è®º] ${tabConclusions[tabId].name} - æ¨¡å¼${patternId}ï¼š${patternName}${sourceCanvasId ? ' (å…³è”å›¾è¡¨:' + sourceCanvasId + ')' : ''}${chartConfig ? ' (å«å›¾è¡¨é…ç½®)' : ''}`);
                }
            }
        }

        // ä»æº canvas å¤åˆ¶å›¾è¡¨åˆ°ç›®æ ‡ canvasï¼ˆå±•å¼€æ—¶è°ƒç”¨ï¼‰
        function copyChartToCanvas(sourceCanvasId, targetCanvasId) {
            const sourceCanvas = document.getElementById(sourceCanvasId);
            const targetCanvas = document.getElementById(targetCanvasId);

            if (!sourceCanvas || !targetCanvas) {
                console.warn(`[å¤åˆ¶å›¾è¡¨] canvas ä¸å­˜åœ¨: source=${sourceCanvasId}, target=${targetCanvasId}`);
                return false;
            }

            try {
                // è®¾ç½®ç›®æ ‡ canvas å°ºå¯¸
                targetCanvas.width = sourceCanvas.width;
                targetCanvas.height = sourceCanvas.height;

                const ctx = targetCanvas.getContext('2d');

                // å¡«å……æ·±è‰²èƒŒæ™¯
                ctx.fillStyle = '#1e293b';
                ctx.fillRect(0, 0, targetCanvas.width, targetCanvas.height);

                // å¤åˆ¶æºå›¾è¡¨
                ctx.drawImage(sourceCanvas, 0, 0);

                console.log(`[å¤åˆ¶å›¾è¡¨] æˆåŠŸ: ${sourceCanvasId} â†’ ${targetCanvasId}`);
                return true;
            } catch (e) {
                console.warn(`[å¤åˆ¶å›¾è¡¨] å¤±è´¥: ${sourceCanvasId}`, e);
                return false;
            }
        }

