            // å›¾è¡¨é…ç½®
            const margin = { top: 30, right: 30, bottom: 40, left: 50 };
            const width = 400 - margin.left - margin.right;
            const height = 280 - margin.top - margin.bottom;

            // æ¯”ä¾‹å°º
            const xScale = (d) => margin.left + (d / 35) * width;
