        function renderSankey(containerId, flows, config = {}) {
            const container = document.getElementById(containerId);
            if (!container || flows.length === 0) return;

            container.innerHTML = '';

            // æ”¶é›†æ‰€æœ‰èŠ‚ç‚¹
            const nodes = new Map();
            flows.forEach(f => {
                if (!nodes.has(f.from)) nodes.set(f.from, { name: f.from, inValue: 0, outValue: 0 });
                if (!nodes.has(f.to)) nodes.set(f.to, { name: f.to, inValue: 0, outValue: 0 });
                nodes.get(f.from).outValue += f.value;
                nodes.get(f.to).inValue += f.value;
            });

            // åˆ†å±‚
            const leftNodes = [...nodes.values()].filter(n => n.inValue === 0 || n.outValue > n.inValue);
            const rightNodes = [...nodes.values()].filter(n => n.outValue === 0 || n.inValue > n.outValue);

            const maxValue = Math.max(...flows.map(f => f.value));

            // æ¸²æŸ“èŠ‚ç‚¹å’Œè¿çº¿
            const flowsHTML = flows.slice(0, 15).map((flow, i) => {
                const thickness = Math.max((flow.value / maxValue) * 40, 8);
