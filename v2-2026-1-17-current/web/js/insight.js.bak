         * @param {string} containerId - å®¹å™¨divçš„ID
         * @param {Array} words - è¯è¯­æ•°æ® [{text, weight, color?}, ...]
         * @param {Object} config - é…ç½®
         */
        function renderWordCloud(containerId, words, config = {}) {
            const container = document.getElementById(containerId);
            if (!container || words.length === 0) return;

            // æ¸…ç©ºå®¹å™¨
            container.innerHTML = '';

            // æ ¹æ®æƒé‡æ’åº
            const sorted = [...words].sort((a, b) => b.weight - a.weight);
            const maxWeight = sorted[0]?.weight || 1;
            const minWeight = sorted[sorted.length - 1]?.weight || 0;

            // åˆ›å»ºè¯äº‘HTML
            const cloudHTML = sorted.slice(0, config.maxWords || 50).map((word, i) => {
                // è®¡ç®—å­—ä½“å¤§å°ï¼ˆ12-48pxï¼‰
                const normalized = (word.weight - minWeight) / (maxWeight - minWeight + 0.01);
                const fontSize = 12 + normalized * 36;

                // è®¡ç®—é¢œè‰²
                let color;
                if (word.color) {
                    color = word.color;
                } else if (i < 3) {
                    color = 'rgba(239, 68, 68, 0.9)';  // Top 3 çº¢è‰²
                } else if (i < 10) {
                    color = 'rgba(249, 115, 22, 0.8)'; // Top 10 æ©™è‰²
                } else if (normalized > 0.5) {
                    color = 'rgba(6, 182, 212, 0.8)';  // é«˜æƒé‡ é’è‰²
                } else {
                    color = 'rgba(148, 163, 184, 0.7)'; // ä½æƒé‡ ç°è‰²
                }

