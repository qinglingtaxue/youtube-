         * @param {number} max - æœ€å¤§å€¼
         * @param {Object} config - é…ç½®
         */
        function renderGauge(canvasId, value, max, config = {}) {
            const ctx = document.getElementById(canvasId);
            if (!ctx) return null;

            destroyChart(canvasId);

            const percentage = Math.min((value / max) * 100, 100);
            const remaining = 100 - percentage;

            // æ ¹æ®ç™¾åˆ†æ¯”é€‰æ‹©é¢œè‰²
            let color;
            if (percentage >= 80) color = 'rgba(34, 197, 94, 0.8)';      // ç»¿è‰²
            else if (percentage >= 60) color = 'rgba(6, 182, 212, 0.8)'; // é’è‰²
            else if (percentage >= 40) color = 'rgba(234, 179, 8, 0.8)'; // é»„è‰²
            else color = 'rgba(239, 68, 68, 0.8)';                        // çº¢è‰²

            const chart = new Chart(ctx, {
                type: 'doughnut',
                data: {
                    datasets: [{
                        data: [percentage, remaining],
                        backgroundColor: [color, 'rgba(51, 65, 85, 0.5)'],
                        borderWidth: 0,
                        cutout: '75%'
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    rotation: -90,
                    circumference: 180,
                    plugins: {
                        legend: { display: false },
                        tooltip: { enabled: false }
                    }
                },
                plugins: [{
                    id: 'gaugeText',
                    afterDraw: (chart) => {
                        const { ctx, chartArea } = chart;
                        const centerX = (chartArea.left + chartArea.right) / 2;
                        const centerY = chartArea.bottom - 20;

                        ctx.save();
                        ctx.textAlign = 'center';
                        ctx.textBaseline = 'middle';
                        ctx.fillStyle = '#e2e8f0';
                        ctx.font = 'bold 24px system-ui';
                        ctx.fillText(`${percentage.toFixed(0)}%`, centerX, centerY);

                        if (config.label) {
                            ctx.font = '12px system-ui';
                            ctx.fillStyle = '#94a3b8';
                            ctx.fillText(config.label, centerX, centerY + 25);
                        }
                        ctx.restore();
                    }
                }]
            });

            chartInstances[canvasId] = chart;
            cacheChartImage(chart, canvasId);
            return chart;
        }

